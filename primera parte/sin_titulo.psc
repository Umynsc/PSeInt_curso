Algoritmo sin_titulo
	definir frase,aux como cadena; 
	definir numfrase,numfrase2 como entero;
	definir frase2 como cadena;
	
	frase <- 'tres tristes tigrs comen triego en un tigral';
	frase2 <- 'jujuy se encuentra en argentina';
	numfrase <- longitud(frase);
	escribir frase ,' ', numfrase;
	escribir 'hola' + ' '+'chau';
	escribir SubCadena(frase,4,20);
	aux <- concatenar (frase,frase2);
	
	escribir aux;
	
	
FinAlgoritmo
